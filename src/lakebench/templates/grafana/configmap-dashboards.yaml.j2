        {
          "collapsed": false,
          "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
          "id": 100,
          "panels": [],
          "title": "Pipeline Status",
          "type": "row"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "fieldConfig": {
            "defaults": {
              "color": {"mode": "thresholds"},
              "mappings": [
                {"options": {"1": {"color": "green", "index": 0, "text": "RUNNING"}}, "type": "value"},
                {"options": {"0": {"color": "dark-gray", "index": 1, "text": "IDLE"}}, "type": "value"}
              ],
              "thresholds": {"mode": "absolute", "steps": [{"color": "dark-gray", "value": null}, {"color": "green", "value": 1}]}
            }
          },
          "gridPos": {"h": 3, "w": 6, "x": 0, "y": 1},
          "id": 1,
          "options": {"colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}, "textMode": "value_and_name"},
          "targets": [{"expr": "count(up{job=\"spark-driver\"} == 1) or vector(0)", "legendFormat": "Spark Drivers", "refId": "A"}],
          "title": "",
          "type": "stat"
        },
