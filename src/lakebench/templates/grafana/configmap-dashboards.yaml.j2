{# Grafana Dashboards ConfigMap for Lakebench Pipeline #}
{# Three dashboards: Pipeline Overview, Spark Detail, Storage I/O #}
apiVersion: v1
kind: ConfigMap
metadata:
  name: lakebench-grafana-dashboards
  namespace: {{ namespace }}
  labels:
    app.kubernetes.io/name: lakebench
    app.kubernetes.io/component: grafana
